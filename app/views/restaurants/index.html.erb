<p id="notice"><%= notice %></p>

<h1>Restaurants</h1>

<table>
  <thead>
    <tr>
      <th>Note eco</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @restaurants.each do |rs| %>
      <tr>
        <td><%= rs.q1.to_i+rs.q2.to_i+rs.q3.to_i+rs.q4.to_i+rs.q5.to_i+rs.q6.to_i+rs.q7.to_i+rs.q8.to_i+rs.q9.to_i+rs.q10.to_i+rs.q11.to_i+rs.q12.to_i %></td>

        <td><%= link_to 'Show', rs %></td>
        <td><%= link_to 'Edit', edit_restaurant_path(rs) %></td>
        <td><%= link_to 'Destroy', rs, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Restaurant', new_restaurant_path %>
